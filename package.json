{
  "name": "cozy-home",
  "version": "1.46.0",
  "main": "src/index.jsx",
  "scripts": {
    "tx": "tx pull --all || true",
    "posttx": "./scripts/check-locales.sh",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "eslint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "build": "yarn build:browser",
    "build:browser": "cozy-scripts build --browser",
    "postbuild:browser": "workbox injectManifest config/workbox.config.js && rm -rf build/serviceWorker",
    "build:mobile": "cozy-scripts build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cozy-scripts start --browser --hot",
    "watch:mobile": "cozy-scripts watch --mobile",
    "start": "cozy-scripts start --hot --browser",
    "cozyPublish": "cozy-scripts publish --token $REGISTRY_TOKEN --prepublish downcloud --postpublish mattermost",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-origin}",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/cozy-home.git"
  },
  "author": "Cozy Cloud <contact@cozycloud.cc> (https://cozy.io/)",
  "contributors": [
    "CPatchane"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/cozy-home/issues"
  },
  "homepage": "https://github.com/cozy/cozy-home#readme",
  "dependencies": {
    "@cozy/minilog": "1.0.0",
    "@material-ui/core": "4.12.3",
    "@material-ui/lab": "4.0.0-alpha.60",
    "cozy-client": "27.26.0",
    "cozy-device-helper": "1.17.0",
    "cozy-doctypes": "1.83.5",
    "cozy-flags": "2.8.4",
    "cozy-harvest-lib": "^7.3.4",
    "cozy-intent": "1.12.0",
    "cozy-keys-lib": "3.11.0",
    "cozy-logger": "1.8.1",
    "cozy-realtime": "4.0.5",
    "cozy-sharing": "4.1.0",
    "cozy-ui": "^62.3.0",
    "date-fns": "2.28.0",
    "form-data": "2.5.1",
    "intro.js-fix-cozy": "2.9.5",
    "leaflet": "1.7.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "papaparse": "5.3.1",
    "piwik-react-router": "0.12.1",
    "prop-types": "^15.8.1",
    "react-inspector": "5.1.1",
    "react-markdown": "4.3.1",
    "react-redux": "7.2.6",
    "react-router-dom": "4.3.1",
    "react-swipeable-views": "0.14.0",
    "redux": "4.1.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "7.0.2",
    "babel-jest": "27.5.1",
    "babel-preset-cozy-app": "2.0.2",
    "cozy-client-js": "0.20.0",
    "cozy-scripts": "6.2.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "8.9.0",
    "eslint-config-cozy-app": "4.0.0",
    "git-directory-deploy": "1.5.1",
    "husky": "1.3.1",
    "ignore-not-found-export-webpack-plugin": "1.0.2",
    "lint-staged": "8.2.1",
    "mockdate": "3.0.5",
    "npm-run-all": "4.1.5",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "16.14.0",
    "remark-preset-lint-recommended": "5.0.0",
    "stylint": "2.0.0",
    "stylus": "0.55.0",
    "url-search-params-polyfill": "8.1.1",
    "workbox-cli": "6.5.1",
    "workbox-precaching": "6.5.1"
  },
  "lint-staged": {
    "{src,test}/**/*.{js,jsx}": [
      "eslint"
    ]
  },
  "resolutions": {
    "scheduler": "0.20.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
