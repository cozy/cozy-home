    "@types/react-dom": "18.0.6",
    "babel-jest": "27.5.1",
    "babel-preset-cozy-app": "2.0.2",
    "bundlemon": "1.3.2",
    "cozy-client-js": "0.20.0",
    "cozy-scripts": "^6.4.0",
    "eslint": "8.9.0",
    "eslint-config-cozy-app": "4.0.0",
