    "react-hot-loader": "^4.13.0",
    "react-test-renderer": "18.2.0",
    "remark-preset-lint-recommended": "5.0.0",
    "stylint": "2.0.0",
    "stylus": "0.55.0",
    "url-search-params-polyfill": "8.1.1"
