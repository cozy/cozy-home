    "cozy-harvest-lib": "^13.12.0",
    "cozy-intent": "^2.3.0",
    "cozy-keys-lib": "^4.3.0",
    "cozy-logger": "1.10.0",
    "cozy-realtime": "4.2.9",
    "cozy-sharing": "7.1.0",
    "cozy-stack-client": "^36.2.0",
    "cozy-tsconfig": "1.2.0",
    "cozy-ui": "^82.1.1",
    "date-fns": "2.28.0",
    "es-abstract": "1.20.2",
    "form-data": "2.5.1",
    "intro.js-fix-cozy": "2.9.5",
    "leaflet": "1.7.1",
